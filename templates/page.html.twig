{% extends "base.html.twig" %}

{% block title %}{{ title }}{% endblock %}

{% block content %}
	<section class="container page">
		<article>
			<header>
				{% block page_header %}
					<h1 class="title">
						<a class="title-link" href="{{ title_link }}">{{ title }}</a>
					</h1>
				{% endblock %}
			</header>
			{% block page_content %}{% endblock %}
		</article>
	</section>
{% endblock %}