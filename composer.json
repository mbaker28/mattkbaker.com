{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "twig/twig": "^2.12|^3.0",
        "vlucas/phpdotenv": "^5.5",
        "php-di/php-di": "^7.0",
        "symfony/http-foundation": "^6.2",
        "nikic/fast-route": "^1.3",
        "erusev/parsedown": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
